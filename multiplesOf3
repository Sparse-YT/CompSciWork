import java.util.ArrayList;
public class multiplesOf3 {
    public static void main(String[] args) {
        ArrayList<Integer> multiplesOf3 = new ArrayList<Integer>();
        for(int i = 0; i<101; i++){
            multiplesOf3.add(3*i);
            System.out.println(multiplesOf3.get(i));
        }
     }
 }
